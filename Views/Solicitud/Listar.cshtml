@model List<Solicitud>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Lista de Usuarios</h1>
    

    @if (Model.Count == 0) {
        <p>No hay Usuarios Registrados</p>
    }
    
    <table class="table">

        <thead class="thead-dark">

           <th>Nombre</th>
           <th>Precio</th>
           <th>categoria</th>
           <th>celular</th>
           <th>comprador</th>
            <th>fecha</th>
             <th>foto</th>
             <th>lugar</th>

        </thead>
    
      <tbody>

    @foreach (var Soli in Model) {
       
       <tr>
       
          <td>@Soli.Nombre</td>
          <td>@Soli.Precio</td>
          <td>@Soli.categoria</td>
          <td>@Soli.Celular</td>
          <td>@Soli.Comprador</td>
          <td>@Soli.Fecha</td>
          <td>@Soli.Foto</td>
          <td>@Soli.Lugar</td>
             
         

       </tr>

    }

    </tbody>

    </table>
</div>